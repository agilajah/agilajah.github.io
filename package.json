{
	"name": "febi-blog",
	"version": "1.0.0",
	"private": false,
	"scripts": {
		"dev": "node --no-warnings node_modules/.bin/astro dev",
		"start": "astro dev",
		"build": "astro build",
		"postbuild": "pagefind --site dist",
		"preview": "astro preview",
		"lint": "biome lint .",
		"format": "pnpm run format:code && pnpm run format:imports",
		"format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
		"format:imports": "biome check --formatter-enabled=false --write",
		"check": "astro check"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"dependencies": {
		"@astrojs/mdx": "3.1.4",
		"@astrojs/rss": "4.0.7",
		"@astrojs/sitemap": "3.1.6",
		"@astrojs/tailwind": "5.1.1",
		"astro": "4.14.3",
		"astro-expressive-code": "^0.35.6",
		"astro-icon": "^1.1.1",
		"cssnano": "^7.0.5",
		"hastscript": "^9.0.0",
		"mdast-util-directive": "^3.0.0",
		"mdast-util-to-markdown": "^2.1.0",
		"mdast-util-to-string": "^4.0.0",
		"rehype-external-links": "^3.0.0",
		"rehype-katex": "^7.0.1",
		"rehype-mathjax": "^6.0.0",
		"remark-directive": "^3.0.0",
		"remark-math": "^6.0.0",
		"satori": "0.10.14",
		"satori-html": "^0.3.2",
		"sharp": "^0.33.5",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.3",
		"@biomejs/biome": "^1.8.3",
		"@iconify-json/mdi": "^1.1.68",
		"@pagefind/default-ui": "^1.1.0",
		"@resvg/resvg-js": "^2.6.2",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.8",
		"@types/eslint": "^8.4.10",
		"@types/hast": "^3.0.4",
		"@types/mdast": "^4.0.4",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"astro-eslint-parser": "^0.9.3",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-astro": "^0.21.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"pagefind": "^1.1.0",
		"postcss": "^8.4.21",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.2",
		"prettier-plugin-astro": "0.7.2",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"reading-time": "^1.5.0",
		"remark-unwrap-images": "^4.0.0",
		"stylelint": "^14.16.1",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-standard": "^29.0.0",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.5.4"
	}
}
